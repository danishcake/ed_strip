{
  "version": "2.0.0",
  "tasks": [
    {
      "type": "cargo",
      "command": "build",
      "problemMatcher": ["$rustc"],
      "group": {
        "isDefault": true,
        "kind": "build"
      },
      "label": "rust: cargo build"
    },
    {
      "type": "cargo",
      "command": "build",
      "args": ["--release"],
      "problemMatcher": ["$rustc"],
      "group": {
        "kind": "build"
      },
      "label": "rust: cargo build release"
    }
  ]
}
